<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg_round_border_et"
    android:layout_width="match_parent"
    android:layout_margin="16dp"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/label_revenue_collection"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Collection  :" />

    <TextView
        android:id="@+id/tv_revenue_collection"
        android:layout_marginTop="16dp"
        android:textColor="@color/black"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@id/label_revenue_collection"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="collection" />
    <TextView
        android:id="@+id/label_revenue_conductor"
        android:layout_marginTop="10dp"
        android:layout_marginStart="16dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_revenue_collection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Conductor :" />

    <TextView
        android:id="@+id/tv_revenue_conductor"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@id/label_revenue_conductor"
        app:layout_constraintTop_toBottomOf="@id/tv_revenue_collection"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="conductor" />





</androidx.constraintlayout.widget.ConstraintLayout>